<% content_for :header_tags do %>
  <%= stylesheet_link_tag "nv.d3.css", plugin: "redmine_irg" %>
  <%= stylesheet_link_tag "index.scss", plugin: "redmine_irg" %>
  <%= javascript_include_tag "d3.min.js", plugin: "redmine_irg" %>
  <%= javascript_include_tag "nv.d3.min.js", plugin: "redmine_irg" %>
